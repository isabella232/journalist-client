.workspace .sidebar
  width: 180px
  background: #1e2129
  color: #fff
  height: 100%
  padding: 10px
  position: absolute
  top: 0
  left: 0

  ul
    list-style-type: none
    padding: 0px
    margin: 0px

    span.profiler-type
      font-size: 80%;
      font-weight: bold;
      color: #9a9a9a
      text-transform: uppercase

    .local-profilers-list
      color: #fff
      padding-left: 5px;

    .local-profiler.selected
      background: #254554
